{
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "prepare": "rm -rf .husky && husky install && husky add .husky/pre-commit \"npx lint-staged\"",
        "format": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|css|scss|md)\"",
        "translate": "node scripts/translate.js"
    },
    "lint-staged": {
        "*.+(js|jsx|ts|tsx)": "eslint --max-warnings 0",
        "*.+(js|jsx|ts|tsx|json|css|md)": "prettier --write"
    },
    "dependencies": {
        "@svgr/webpack": "^6.2.1",
        "axios": "^0.27.2",
        "next": "latest",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "swiper": "^8.3.1",
        "next-i18next": "^11.0.0"
    },
    "devDependencies": {
        "@types/node": "^12.12.21",
        "@types/react": "^17.0.2",
        "@types/react-dom": "^17.0.1",
        "autoprefixer": "^10.4.7",
        "eslint": "8.14.0",
        "eslint-config-next": "12.1.5",
        "eslint-config-prettier": "^8.5.0",
        "husky": "^7.0.4",
        "lint-staged": "^12.4.1",
        "postcss": "^8.4.14",
        "prettier": "^2.6.2",
        "sass": "^1.51.0",
        "tailwindcss": "^3.0.24",
        "typescript": "4.0"
